<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <HediffDef ParentName="RV2_PreyInTailmaw">
      <defName>RV2_PreyInTailmawDumping</defName>
      <description>This pawn is spitting the remains of prey out of their tailmaw.</description>
    </HediffDef>

    <!-- Tailthroat -->
    <HediffDef ParentName="RV2_PreyInTailthroat">
        <defName>RV2_PreyInTailThroatWarming</defName>
        <description>This pawn is preparing to do something to a pawn in their tail-throat.</description>
    </HediffDef>
    <HediffDef ParentName="RV2_PreyInTailthroat">
        <defName>RV2_PreyInTailThroatDigesting</defName>
        <description>This pawn is digesting a pawn in their tail-throat.</description>
    </HediffDef>
    <HediffDef ParentName="RV2_PreyInTailthroat">
        <defName>RV2_PreyInTailThroatHealing</defName>
        <description>This pawn is healing a pawn in their tail-throat.</description>
    </HediffDef>
     <HediffDef ParentName="RV2_PreyInTailthroat">
        <defName>RV2_PreyInTailThroatContacting</defName>
        <description>This pawn is absorbing prey with their tail-throat.</description>
    </HediffDef>
    <HediffDef ParentName="RV2_PreyInTailthroat">
        <defName>RV2_PreyInTailthroatLoosening</defName>
        <description>This pawn is warming up and loosening their tail-throat for absorption.</description>
    </HediffDef>
    <HediffDef ParentName="RV2_PreyInTailthroat">
        <defName>RV2_PreyInTailThroatAmalgamating</defName>
        <description>This pawn is storing absorbed prey into their tail-throat.</description>
    </HediffDef>
    <HediffDef ParentName="RV2_PreyInTailthroat">
        <defName>RV2_PreyInTailThroatAssimilating</defName>
        <description>This pawn is fully-absorbing their prey into their tail-throat.</description>
    </HediffDef>
    <HediffDef ParentName="RV2_PreyInTailthroat">
        <defName>RV2_PreyInTailthroatPurging</defName>
        <description>This pawn is removing any unassimilated remains from their tail-throat.</description>
    </HediffDef>
    <HediffDef ParentName="RV2_PreyInTailthroat">
        <defName>RV2_PreyInTailthroatReshaping</defName>
        <description>This pawn is reshaping prey stored as a part of tail-throat for release.</description>
    </HediffDef>


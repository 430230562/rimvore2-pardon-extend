<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
  <AbilityDef ParentName="RV2_VoreSkip_Base">
    <defName>RV2_Ability_VoreSkip</defName>
    <label>Vore Skip</label>
    <description>Teleport the target into another target (either can be self). Body part and vore goal are selected randomly.</description>
    <iconPath>UI/Icons/Abilities/VoreSkip</iconPath>
    <modExtensions>
      <li Class="RimVore2.AbilityExtension_VoreSkip"/>
    </modExtensions>
    <statBases>
      <Ability_EntropyGain>0</Ability_EntropyGain>
      <Ability_PsyfocusCost>0</Ability_PsyfocusCost>
    </statBases>
    <verbProperties>
      <verbClass>RimVore2.Verb_CastPsycast_MultiTarget</verbClass>
      <warmupTime>0.05</warmupTime>
      <range>19.9</range>
      <targetParams>
        <canTargetSelf>True</canTargetSelf>
        <canTargetAnimals>True</canTargetAnimals>
      </targetParams>
    </verbProperties>
    <cooldownTicksRange>1200</cooldownTicksRange>
  </AbilityDef>
</Defs>
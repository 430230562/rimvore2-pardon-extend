<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <!-- 其他 -->
    
    <GeneDef>
        <DefName>RV2_Ability_VoreSkip</DefName>
        <label>Vore Skip</label>
        <description>Carriers can teleport a target into another target (either can be self). Body part and vore goal are selected randomly.</description>
        <iconPath>UI/Icons/Abilities/VoreSkip</iconPath>
        <displayCategory>Ability</displayCategory>
        <abilities>
            <li>RV2_Ability_VoreSkip</li>
        </abilities>
        <descriptionHyperlinks>
            <AbilityDef>RV2_Ability_VoreSkip</AbilityDef>
        </descriptionHyperlinks>
        <biostatMet>-2</biostatMet>
    </GeneDef>

    <!-- 机动性 -->
    <GeneDef>
        <defName>RV2_QuirkForce_Cheat_Hammerspace</defName>
        <label>Hammerspace</label>
        <description>This pawn’s body compresses its prey down to an incredible degree. You can’t quite tell how many prey has inside this pawn at any given time, though you can guess if you know its limit.</description>
        <geneClass>RimVore2.Gene_ForcedHediff</geneClass>
        <iconPath>UI/Icons/Genes/Mobility_Cheat</iconPath>
        <modExtensions>
            <li Class="RimVore2.GeneDefExtension_ForcedHediff">
                <hediffs>
                    <li>RV2_QuirkForce_Cheat_Hammerspace</li>
                </hediffs>
                <removeHediffsOnGeneRemoval>true</removeHediffsOnGeneRemoval>
            </li>
        </modExtensions>
        <exclusionTags>
            <li>Mobility</li>
        </exclusionTags>
        <displayCategory>RV2_Vore_Capacity</displayCategory>
        <displayOrderInCategory>0</displayOrderInCategory>
        <biostatMet>0</biostatMet>
        <biostatArc>1</biostatArc>
    </GeneDef>


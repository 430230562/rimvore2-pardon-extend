  <ThingDef ParentName="RV2_Product_Consumable_Base" MayRequire='MisTF.Piss'>
    <defName>RV2_Resource_Piss</defName>
    <label>Piss</label>
    <description>The golden remnants of a urified living being.\nYou can make fertilizer out of this at a crafting spot.</description>
    <graphicData>
      <texPath>Things/VoreProducts/Piss</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <thingCategories>
      <li>RV2_FertilizerIngredient</li>
      <li>RV2_VoreProduct</li>
    </thingCategories>
    <comps Inherit="False">
      <li Class="CompProperties_Forbiddable" />
    </comps>
    <statBases>
      <Nutrition>0.05</Nutrition>
    </statBases>
  </ThingDef>

  <ThingDef ParentName="RV2_Product_Consumable_Base" MayRequire='MisTF.Scat'>
    <defName>RV2_Resource_Scat</defName>
    <label>Scat</label>
    <description>The reeking remnants of a digested living being.\nYou can make fertilizer out of this at a crafting spot.</description>
    <graphicData>
      <texPath>Things/VoreProducts/Scat</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <thingCategories>
      <li>RV2_FertilizerIngredient</li>
      <li>RV2_VoreProduct</li>
    </thingCategories>
    <statBases>
      <Nutrition>0.25</Nutrition>
    </statBases>
  </ThingDef>

</Defs>